#include<iostream>
using namespace std;

int main() {
	cout<<"enter the no to search: ";
	int target;
	cin>>target;
	int arr[] = {3,6,5,4,7,8,5,2,9,6,4};
	int index = -1;
	int n = sizeof(arr)/sizeof(arr[0]);
	for(int i=0; i<n; i++) {
		if(arr[i]==target) {
			index=i;
			break;
		}
	}
	if(index != -1) {
		cout<<"Number "<<target <<" is present at index : "<<index<<endl;
	}
	else {
		cout<<"number "<<target <<" is not present  "<<index;
	}
}
