#include<iostream>
using namespace std;

int main() {
cout<<"enter 5 number ";
int n=5;
 int arr[5];
 for(int i=0;i<n;i++){
      cin>>arr[i];
 }
 bool isPalindrome =true;
 for(int i=0;i<n/2;i++){
     if(arr[i] != arr[n-1-i]){
         isPalindrome = false;
         break;
     }
}
if(isPalindrome){
    cout<<"palindrome number";
}
     else{
         cout<<" Not palindrome number";
     }
 
}
